filetype plugin on
syntax on
set nocompatible
set number
set showmatch
set showmode
set visualbell
set smartcase
set ignorecase
set incsearch
set autoindent
set expandtab
set shiftwidth=2
set smartindent
set smarttab
set softtabstop=2
set confirm
set ruler
set wildmenu
set undolevels=1000
set backspace=indent,eol,start

